{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/blockylicious",
  "version": "0.1.0",
  "title": "Blockylicious",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "link": true
    },
    "spacing": {
      "padding": true
    }
  },
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "#ec4899",
          "text": "black",
          "link": "blue"
        },
        "spacing": {
          "padding": {
            "top": "80px",
            "bottom": "80px",
            "left": "50px",
            "right": "50px"
          }
        }
      }
    },
    "enableTopCurve": {
      "type": "boolean",
      "default": true
    },
    "width": {
      "type": "number",
      "default": 100
    },
    "height": {
      "type": "number",
      "default": 100
    },
    "flipX": {
      "type": "boolean",
      "default": false
    },
    "flipY": {
      "type": "boolean",
      "default": false
    },
    "color": {
      "type": "object",
      "default": {
        "background": "#fff"
      }
    },
    "enableBottomCurve": {
      "type": "boolean",
      "default": false
    },
    "bottomWidth": {
      "type": "number",
      "default": 100
    },
    "bottomHeight": {
      "type": "number",
      "default": 100
    },
    "bottomFlipX": {
      "type": "boolean",
      "default": false
    },
    "bottomFlipY": {
      "type": "boolean",
      "default": false
    },
    "bottomColor": {
      "type": "object",
      "default": {
        "background": "#fff"
      }
    }
  },
  "textdomain": "blockylicious",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}